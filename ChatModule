function chatmodule.mathrandom(min,max)

	if min==nil then
		----print("Min Integer is nil converted to 0")
		min=0

	end
	if max==nil then
	--	--print("Max Integer is nil converted to 1")
		max=1
	end
	return 	globalrng:NextInteger(min, max)
end

